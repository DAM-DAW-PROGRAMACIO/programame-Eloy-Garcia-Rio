package Easy;

import java.util.Scanner;

public class p239 {

	public static void main(String[] args) throws Exception {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner (System.in);
		int dias = sc.nextInt();
		int emisoras = sc.nextInt();
		
		while (dias > 0 && emisoras > 0) {
			
			int placeholder = 0;
			int resultado = dias * 24 * 6 * emisoras;
			
			int segundos = resultado % 60;
			placeholder = resultado / 60;
			
		}
	}
}
